{
    "version": 2,
    "public": false,
    "builds": [
        {
            "src": "backend_server/backend_server.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "backend_server/backend_server.py"
        },
        {
            "src": "/get_file",
            "dest": "backend_server/backend_server.py"
        },
        {
            "src": "/docs",
            "dest": "backend_server/backend_server.py"
        },
        {
            "src": "/openapi.json",
            "dest": "backend_server/backend_server.py"
        }
    ]
}